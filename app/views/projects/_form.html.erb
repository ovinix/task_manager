<%= form_for(@project) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.hidden_field :user_id, value: current_user.id, class: 'form-control' %>

  <%= f.label :title %><br>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>